{
    "name": "gatenbysanderson/pdf-generator-sdk",
    "description": "PHP SDK for communicating with the PDF Generator",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mubeen Inamdar",
            "email": "mubeen.inamdar@gatenbysanderson.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:gatenbysanderson/code-sniffer.git",
            "no-api": true
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "GatenbySanderson\\PdfGeneratorSdk\\": "src/",
            "GatenbySanderson\\PdfGeneratorSdk\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "vlucas/phpdotenv": "^2.4",
        "gatenbysanderson/code-sniffer": "dev-develop"
    },
    "scripts": {
        "gscs": "phpcs --standard=vendor/gatenbysanderson/code-sniffer/src/phpcs.xml src"
    }
}
